@{
    ViewData["Title"] = "Home Page";
}

<head>
    <link rel="stylesheet" href="~/css/Navbar.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/Faq.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/Admin.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/js/bootstrap.min.js" integrity="sha384-kjU+l4N0Yf4ZOJErLsIcvOU2qSb74wXpOhqTvwVx3OElZRweTnQ6d31fXEoRD1Jy" crossorigin="anonymous"></script>

</head>

    <h1 class="display-4"></h1>
    
    <h6 class="Text-h6">Want to feel fresh?</h6>
    <section><h3 class="Text-h3">Welcome to Ice Core Home</h3>
        <partial name="Welcome"/>
    </section>
    
    <br id="pick&pay"><h6 class="Text-h6">Right Time</h6>
    <section><h3 class="Text-h3">Pick & Pay</h3>
        <partial name="PickPay"/>
    </section>
    
    <br id="subscriber"><h6 class="Text-h6">Welcome Back</h6>
    <section><h3 class="Text-h3">Vanilla Subscriber</h3>
        <partial name="SubscriberForm"/>
    </section>
    
    <br id="become-subs"><h6 class="Text-h6">Want to be our part?</h6>
    <section ><h3 class="Text-h3">Become a Subscriber</h3>
        <div class="col signup-sub">
            <div class="row signup-outer-wrap Theme"><div class="signup-side-head-upper signup-side-head"></div>
	            <form method="post" action="/Subscriber/Signup">
                    <partial name="Signup"/>
                </form>
            </div>
        </div>
    </section>
    
    <br id="admin"><h6 class="Text-h6">Here you're</h6>
    <section><h3 class="Text-h3">Nice to meet you Admin</h3>
        <partial name="AdminForm"/>
    </section>
    
    <br  id="updates"><h6 class="Text-h6">Updates on</h6>
    <section><h3 class="Text-h3">What's New</h3>
        <partial name="WhatsNew"/>    
    </section>
    
    <br id="faqs"><h6 class="Text-h6">Instant ask</h6>
    <section><h3 class="Text-h3">FAQs</h3>
        <partial name="FAQs"/>
    </section>
<partial name="Navbar"/>